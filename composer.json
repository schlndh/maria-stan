{
	"name": "schlndh/maria-stan",
	"type": "phpstan-extension",
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"MariaStan\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"MariaStan\\": "tests/"
		}
	},
	"authors": [
		{
			"name": "schldnh"
		}
	],
	"require": {
		"php": "^8.1",
		"ext-mysqli": "*",
		"greenlion/php-sql-parser": "^4.5"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5",
		"squizlabs/php_codesniffer": "^3.7",
		"slevomat/coding-standard": "^8.1",
		"phpstan/phpstan": "^1.8",
		"phpstan/phpstan-phpunit": "^1.1"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
