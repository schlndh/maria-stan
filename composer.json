{
	"name": "schlndh/maria-stan",
	"description": "Static analyser for MariaDB queries",
	"type": "phpstan-extension",
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"MariaStan\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"MariaStan\\": "tests/"
		}
	},
	"authors": [
		{
			"name": "schldnh"
		}
	],
	"require": {
		"php": "^8.1",
		"ext-mysqli": "*",
		"ext-mbstring": "*",
		"phpstan/phpstan": "^1.8"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5",
		"squizlabs/php_codesniffer": "^3.7",
		"slevomat/coding-standard": "~8.5.2",
		"phpstan/phpstan-phpunit": "^1.1",
		"nette/schema": "^1.2"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"extra": {
		"phpstan": {
			"includes": [
				"extension.neon"
			]
		}
	}
}
