includes:
	- vendor/phpstan/phpstan-phpunit/extension.neon
parameters:
	level: 6
	paths:
		- src
		- tests
	treatPhpDocTypesAsCertain: false
	excludePaths:
		- tests/*/data/*
	exceptions:
		check:
			missingCheckedExceptionInThrows: true
	ignoreErrors:
		-
			message: '#.*throws checked exception.*#'
			path: tests/
